<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="frontend.DynamicBoardController"
            stylesheets="/fxml/board.css"
            prefHeight="400.0" prefWidth="600.0">
    <HBox>
        <VBox>
            <Pane fx:id="topLeftBoard"></Pane>
            <VBox fx:id="leftBoard" id="leftBoard"></VBox>
            <Pane fx:id="bottomLeftBoard"></Pane>
        </VBox>
        <VBox>
            <HBox fx:id="topBoard" id="topBoard">
            </HBox>
            <Pane id="css_middle" fx:id="middle">
            </Pane>
            <HBox fx:id="bottomBoard" id="bottomBoard">
            </HBox>
        </VBox>
        <VBox>
            <VBox fx:id="topRightBoard"></VBox>
            <VBox fx:id="rightBoard" id="rightBoard"></VBox>
            <VBox fx:id="bottomRightBoard"></VBox>
        </VBox>
    </HBox>
</AnchorPane>
